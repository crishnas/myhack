def sum_all_substrings(text: str) -> int:
    total = 0
    n = len(text)
    for i in range(n):
        for l in range(1, min(10, n - i) + 1):
            num_str = text[i:i+l].lstrip("0")
            if num_str:  # Exclude zeros and empty
                total += int(num_str)
    return total

# Run
input_text = "12303"
print("âœ… Total:", sum_all_substrings(input_text))  # Output: 16566
